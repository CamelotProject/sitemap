{
    "name": "camelot/sitemap",
    "description": "Provides a way to create/generate a sitemap.",
    "keywords": ["sitemap", "generator", "seo"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KÃ©vin Gomez",
            "email": "contact@kevingomez.fr",
            "homepage": "http://www.kevingomez.fr/"
        },
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "nikic/iter": "^2.0",
        "symfony/property-access": "^4.3 || ^5.0"
    },
    "require-dev": {
        "camelot/coding-style": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16.1",
        "phpunit/phpunit": "^8.4",
        "symfony/browser-kit": "^4.3 || ^5.0",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "symfony/phpunit-bridge": "^4.3 || ^5.0",
        "symfony/routing": "^4.3 || ^5.0",
        "symfony/stopwatch": "^4.3 || ^5.0",
        "symfony/yaml": "^4.3 || ^5.0"
    },
    "autoload": {
        "psr-4": { "Camelot\\Sitemap\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Camelot\\Sitemap\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
